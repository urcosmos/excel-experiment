{
  "parser": "@babel/eslint-parser",
  "parserOptions": {
    "babelOptions": {
      "configFile": "./babel.config.json"
    }
  },
  "env": {
    "browser": true,
    "node": true,
    "es6": true
  },
  "extends": ["eslint:recommended", "google"],
  "rules": {
    "linebreak-style": "off",
    "indent": "off",
    "operator-linebreak": "off",
    "require-jsdoc": "off",
    "object-curly-spacing": "off",
    "new-cap": "off",
    "space-before-function-paren": "off"
  }
}
